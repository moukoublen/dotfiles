#!/usr/bin/env bash

echo -e "\e[0;32morg.gnome.desktop.interface\e[0m \e[0;93mgtk-theme\e[0m          : \e[0;95m$(gsettings get org.gnome.desktop.interface gtk-theme)\e[0m"
echo -e "\e[0;32morg.gnome.desktop.wm.preferences\e[0m \e[0;93mtheme\e[0m         : \e[0;95m$(gsettings get org.gnome.desktop.wm.preferences theme)\e[0m"
echo -e "\e[0;32morg.gnome.shell.extensions.user-theme\e[0m \e[0;93mname\e[0m     : \e[0;95m$(gsettings get org.gnome.shell.extensions.user-theme name)\e[0m"
echo -e "\e[0;32morg.gnome.desktop.interface\e[0m \e[0;93micon-theme\e[0m         : \e[0;95m$(gsettings get org.gnome.desktop.interface icon-theme)\e[0m"
echo -e "\e[0;32morg.gnome.desktop.interface\e[0m \e[0;93mcursor-theme\e[0m       : \e[0;95m$(gsettings get org.gnome.desktop.interface cursor-theme)\e[0m"
echo -e "\e[0;32morg.gnome.desktop.interface\e[0m \e[0;93mcursor-size\e[0m        : \e[0;95m$(gsettings get org.gnome.desktop.interface cursor-size)\e[0m"

echo ""
echo -e "\e[0;37mFonts\e[0m"
echo -e "\e[0;32morg.gnome.desktop.interface\e[0m \e[0;93mfont-name\e[0m          : \e[0;95m$(gsettings get org.gnome.desktop.interface font-name)\e[0m"
echo -e "\e[0;32morg.gnome.desktop.interface\e[0m \e[0;93mdocument-font-name\e[0m : \e[0;95m$(gsettings get org.gnome.desktop.interface document-font-name)\e[0m"
echo -e "\e[0;32morg.gnome.desktop.interface\e[0m \e[0;93mmonospace-font-name\e[0m: \e[0;95m$(gsettings get org.gnome.desktop.interface monospace-font-name)\e[0m"
echo -e "\e[0;32morg.gnome.desktop.wm.preferences\e[0m \e[0;93mtitlebar-font\e[0m : \e[0;95m$(gsettings get org.gnome.desktop.wm.preferences titlebar-font)\e[0m"
